<Style xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=netstandard">
	<Style.Resources>
		<!-- Colors -->
		<Color x:Key="_hyperlinkColor">#FF0074CC</Color>
		<Color x:Key="_hyperlinkColorHover">#FF0A95FF</Color>
		<Color x:Key="_hyperlinkColorPressed">#FF0A95FF</Color>
		<StaticResource x:Key="HyperlinkColor" ResourceKey="_hyperlinkColor" />
		<StaticResource x:Key="HyperlinkColorHover" ResourceKey="_hyperlinkColorHover" />
		<StaticResource x:Key="HyperlinkColorPressed" ResourceKey="_hyperlinkColorPressed" />

		<Color x:Key="_themePrimary">#0078D7</Color>
		<Color x:Key="_themeDark">#005A9E</Color>
		<StaticResource x:Key="ThemePrimary" ResourceKey="_themePrimary" />
		<StaticResource x:Key="ThemeDark" ResourceKey="_themeDark" />
		
	</Style.Resources>
</Style>